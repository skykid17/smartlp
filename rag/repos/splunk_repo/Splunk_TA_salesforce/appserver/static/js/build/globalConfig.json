                {
                    "name": "proxy",
                    "entity": [
                        {
                            "type": "checkbox",
                            "label": "Enable Proxy",
                            "field": "proxy_enabled"
                        },
                        {
                            "type": "singleSelect",
                            "label": "Proxy Type",
                            "options": {
                                "disableSearch": true,
                                "autoCompleteFields": [
                                    {
                                        "value": "http",
                                        "label": "http"
                                    },
                                    {
                                        "value": "socks5",
                                        "label": "socks5"
                                    }
                                ]
                            },
                            "defaultValue": "http",
                            "field": "proxy_type",
                            "required": true
                        },
                        {
                            "type": "text",
                            "label": "Proxy Host",
                            "validators": [
                                {
                                    "type": "string",
                                    "errorMsg": "Maximum length allowed for host is 4096",
                                    "minLength": 1,
                                    "maxLength": 4096
                                },
                                {
                                    "pattern": "^[a-zA-Z0-9:][a-zA-Z0-9\\.\\-:]+$",
                                    "errorMsg": "Proxy Host is invalid",
                                    "type": "regex"
                                }
                            ],
                            "field": "proxy_url",
                            "required": true
                        },
                        {
                            "type": "text",
                            "label": "Proxy Port",
                            "validators": [
                                {
                                    "type": "number",
                                    "range": [
                                        1,
                                        65535
                                    ]
                                },
                                {
                                    "type": "regex",
                                    "errorMsg": "Proxy Port field must be an integer.",
                                    "pattern": "^\\d+$"
                                }
                            ],
                            "field": "proxy_port",
                            "required": true
                        },
                        {
                            "type": "text",
                            "label": "Proxy Username",
                            "validators": [
                                {
                                    "type": "string",
                                    "errorMsg": "Maximum length allowed for username is 50",
                                    "minLength": 0,
                                    "maxLength": 50
                                }
                            ],
                            "field": "proxy_username"
                        },
                        {
                            "type": "text",
                            "label": "Proxy Password",
                            "validators": [
                                {
                                    "type": "string",
                                    "errorMsg": "Maximum length allowed for password is 8192",
                                    "minLength": 0,
                                    "maxLength": 8192
                                }
                            ],
                            "encrypted": true,
                            "field": "proxy_password"
                        },
                        {
                            "type": "checkbox",
                            "label": "Use Proxy for DNS Resolution",
                            "field": "proxy_rdns"
                        }
                    ],
                    "title": "Salesforce Proxy Setup"
                },
                {
                    "name": "logging",
                    "title": "Logging",
                    "entity": [
                        {
                            "type": "singleSelect",
                            "label": "Log level",
                            "options": {
                                "disableSearch": true,
                                "autoCompleteFields": [
                                    {
                                        "value": "DEBUG",
                                        "label": "DEBUG"
                                    },
                                    {
                                        "value": "INFO",
                                        "label": "INFO"
                                    },
                                    {
                                        "value": "WARNING",
                                        "label": "WARNING"
                                    },
                                    {
                                        "value": "ERROR",
                                        "label": "ERROR"
                                    },
                                    {
                                        "value": "CRITICAL",
                                        "label": "CRITICAL"
                                    }
                                ]
                            },
                            "defaultValue": "INFO",
                            "field": "loglevel",
                            "required": true,
                            "validators": [
                                {
                                    "type": "regex",
                                    "errorMsg": "Log level must be one of: DEBUG, INFO, WARNING, ERROR, CRITICAL",
                                    "pattern": "^DEBUG|INFO|WARNING|ERROR|CRITICAL$"
                                }
                            ],
                            "help": "(DEBUG, INFO, WARN, ERROR or CRITICAL)"
                        }
                    ]
                }
            ],
            "title": "Configuration",
            "description": "Configure your Salesforce credentials, Proxy server details and log level"
