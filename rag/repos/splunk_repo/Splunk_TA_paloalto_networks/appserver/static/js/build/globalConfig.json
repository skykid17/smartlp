                                        "label": "EU1"
                                    },
                                    {
                                        "value": "uk",
                                        "label": "UK"
                                    },
                                    {
                                        "value": "in1",
                                        "label": "INDIA"
                                    },
                                    {
                                        "value": "au1",
                                        "label": "AUSTRALIA"
                                    },
                                    {
                                        "value": "uk2",
                                        "label": "UK2"
                                    },
                                    {
                                        "value": "jp1",
                                        "label": "JAPAN"
                                    }
