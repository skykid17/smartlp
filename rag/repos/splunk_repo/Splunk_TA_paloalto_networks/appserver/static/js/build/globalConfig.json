{
    "pages": {
        "configuration": {
            "title": "Configuration",
            "description": "Set up your add-on",
            "tabs": [
                {
                    "name": "iot_account",
                    "title": "IoT accounts",
                    "table": {
                        "actions": [
                            "edit",
                            "delete",
                            "clone"
                        ],
                        "header": [
                            {
                                "label": "Customer ID",
                                "field": "name"
                            }
                        ]
                    },
                    "entity": [
                        {
                            "type": "text",
                            "label": "Customer ID",
                            "help": "Found in the hostname when accessing IoT Security. (eg. https://customer-id.iot.paloaltonetworks.com)",
                            "field": "name",
                            "required": true
                        },
                        {
                            "type": "text",
                            "label": "Access Key ID",
                            "help": "Access Key ID created in IoT security dashboard.",
                            "field": "access_key_id",
                            "encrypted": true,
                            "required": true
                        },
                        {
                            "type": "text",
                            "label": "Secret Access Key",
                            "help": "Secret Access Key created in IoT security dashboard.",
                            "field": "secret_access_key",
                            "encrypted": true,
                            "required": true
                        }
                    ]
                },
                {
                    "name": "data_security_account",
