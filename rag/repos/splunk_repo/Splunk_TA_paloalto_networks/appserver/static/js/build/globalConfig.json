    "alerts": [
        {
            "name": "alert_pantag",
            "label": "Palo Alto Networks Tag to Dynamic",
            "description": "Add/remove Palo Alto Networks dynamic address or user group tag",
            "entity": [
                {
                    "type": "text",
                    "label": "Hostname",
                    "field": "hostname",
                    "help": "IP or hostname of Firewall/Panorama to apply tag. Make sure that account with the same hostname is configured in the add-on's Firewall & Panorama section.",
                    "required": true
                },
                {
                    "type": "singleSelect",
                    "label": "Action",
                    "field": "action",
                    "help": "Add or Remove the tag.",
                    "required": true,
                    "defaultValue": "add_ip",
                    "options": {
                        "items": [
                            {
                                "value": "add_ip",
                                "label": "Add IP"
                            },
                            {
                                "value": "remove_ip",
                                "label": "Remove IP"
                            },
                            {
                                "value": "add_user",
                                "label": "Add User"
                            },
                            {
                                "value": "remove_user",
                                "label": "Remove User"
                            }
                        ],
                        "disableSearch": true
                    }
                },
                {
                    "type": "text",
                    "label": "Tags",
                    "field": "tags",
                    "help": "Tags to add or remove (use spaces to separate multiple tags)",
                    "required": true
                },
                {
                    "type": "text",
                    "label": "Timeout",
                    "field": "timeout",
                    "required": false,
                    "help": "Specify timeout to unregister tag mapping after a specified amount of time. If no timeout is specified, the tag mapping should be unregistered manually."
                }
            ]
        }
    ]
}
