            "services": [
                {
                    "template": "input_with_helper",
                    "name": "palo_iot_security",
                    "title": "IoT Security",
                    "entity": [
                        {
                            "field": "name",
                            "label": "Name",
                            "type": "text",
                            "help": "Enter a unique name for the data input",
                            "required": true,
                            "validators": [
                                {
                                    "type": "regex",
                                    "pattern": "^[a-zA-Z]\\w*$",
                                    "errorMsg": "Input Name must start with a letter and followed by alphabetic letters, digits or underscores."
                                },
                                {
                                    "type": "string",
                                    "minLength": 1,
                                    "maxLength": 100,
                                    "errorMsg": "Length of input name should be between 1 and 100"
                                }
                            ]
                        },
                        {
                            "type": "text",
                            "field": "interval",
                            "label": "Interval",
                            "validators": [
                                {
                                    "type": "regex",
                                    "errorMsg": "Interval must be either a non-negative number, CRON interval or -1.",
                                    "pattern": "^((?:-1|\\d+(?:\\.\\d+)?)|(([\\*\\d{1,2}\\,\\-\\/]+\\s){4}[\\*\\d{1,2}\\,\\-\\/]+))$"
                                }
                            ],
                            "help": "Time interval of input in seconds.",
                            "required": true
                        },
                        {
                            "field": "index",
                            "label": "Index",
                            "type": "singleSelect",
                            "defaultValue": "default",
                            "help": "Index where data is going to be ingested.",
                            "options": {
                                "endpointUrl": "data/indexes",
                                "createSearchChoice": true,
                                "denyList": "^_.*$"
                            },
                            "required": true,
                            "validators": [
                                {
                                    "type": "string",
                                    "minLength": 1,
                                    "maxLength": 80,
                                    "errorMsg": "Length of index name should be between 1 and 80."
                                }
                            ]
                        },
                        {
                            "type": "singleSelect",
                            "label": "IoT account",
                            "options": {
                                "referenceName": "iot_account"
                            },
                            "help": "IoT account to use for this input.",
                            "field": "iot_account",
                            "required": true
                        },
                        {
                            "field": "start_time",
                            "label": "Collection date time start",
                            "type": "text",
                            "help": "Specify a date and time in UTC format (YYYY-MM-DD HH:MM:SS) from which to start collecting data. For example, 2024-03-10 09:35:00",
                            "required": true,
                            "validators": [
                                {
                                    "type": "regex",
                                    "pattern": "^\\d{4}-\\d{2}-\\d{2} \\d{2}:\\d{2}:\\d{2}$",
                                    "errorMsg": "Expects UTC datetime value YYYY-MM-DD HH:MM:SS, for example 2024-03-10 09:35:00"
                                }
                            ]
                        }
                    ]
                },
