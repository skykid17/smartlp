{
    "pages": {
        "configuration": {
            "tabs": [
                {
                    "restHandlerModule": "Splunk_TA_SAA_rh_account_post_configure",
                    "restHandlerClass": "ConnectionSetupPostConfigure",
                    "name": "account",
                    "table": {
                        "actions": [
                            "edit",
                            "delete",
                            "clone"
                        ],
                        "header": [
                            {
                                "label": "Name",
                                "field": "name"
                            },
                            {
                                "label": "API URL",
                                "field": "base_url"
                            }
                        ]
                    },
                    "entity": [
                        {
                            "type": "text",
                            "label": "Name",
                            "validators": [
                                {
                                    "type": "regex",
                                    "errorMsg": "Account Name must begin with a letter and consist exclusively of alphanumeric characters and underscores.",
                                    "pattern": "^[a-zA-Z]\\w*$"
                                },
                                {
                                    "type": "string",
                                    "errorMsg": "Length of input name should be between 1 and 100",
                                    "minLength": 1,
                                    "maxLength": 100
                                }
                            ],
                            "field": "name",
                            "help": "A unique name for the data input.",
                            "required": true
                        },
                        {
                            "type": "text",
                            "label": "API URL",
                            "field": "base_url",
                            "required": true,
                            "defaultValue": "https://api.twinwave.io",
                            "help": "Attack Analyzer API URL. This should match your regions assigned API URL.",
                            "validators": [
                                {
                                    "type": "regex",
                                    "pattern": "^https://.*$",
                                    "errorMsg": "API URL must start with https://"
                                },
                                {
                                    "type": "regex",
                                    "pattern": "^https:\\/\\/api(?:\\.[a-zA-Z0-9-]+)*\\.twinwave\\.io$",
                                    "errorMsg": "Provided API URL does not seem to be a valid SAA API URL"
                                }
                            ]
                        },
                        {
                            "type": "text",
                            "label": "API Key",
                            "field": "api_key",
                            "help": "Splunk Attack Analyzer API Key",
                            "required": true,
                            "encrypted": true
                        }
                    ],
                    "title": "Connections"
                },
                {
                    "name": "logging",
                    "title": "Logging",
                    "entity": [
                        {
                            "type": "singleSelect",
                            "label": "Log level",
                            "options": {
                                "disableSearch": true,
                                "autoCompleteFields": [
                                    {
                                        "value": "DEBUG",
                                        "label": "DEBUG"
                                    },
                                    {
                                        "value": "INFO",
                                        "label": "INFO"
                                    },
                                    {
                                        "value": "WARN",
                                        "label": "WARN"
                                    },
                                    {
                                        "value": "ERROR",
                                        "label": "ERROR"
                                    },
                                    {
                                        "value": "CRITICAL",
                                        "label": "CRITICAL"
                                    }
                                ]
                            },
                            "defaultValue": "INFO",
                            "field": "loglevel",
                            "required": true,
                            "validators": [
                                {
                                    "type": "regex",
                                    "errorMsg": "Log level must be one of: DEBUG, INFO, WARN, ERROR, CRITICAL",
                                    "pattern": "^DEBUG|INFO|WARN|ERROR|CRITICAL$"
                                }
                            ],
                            "help": "The verbosity of the logging done by the add-on. This includes logs written as part of the modular input as well as alert action executions."
                        }
                    ]
                },
                {
                    "name": "proxy",
                    "title": "Proxy",
                    "entity": [
                        {
                            "field": "proxy_enabled",
                            "label": "Enable",
                            "type": "checkbox"
                        },
                        {
                            "field": "proxy_type",
                            "label": "Proxy Type",
                            "type": "singleSelect",
                            "options": {
                                "disableSearch": true,
                                "autoCompleteFields": [
                                    {
                                        "value": "http",
                                        "label": "http"
                                    },
                                    {
                                        "value": "https",
                                        "label": "https"
                                    }
                                ]
                            }
                        },
                        {
                            "field": "host",
                            "label": "Host",
                            "type": "text",
                            "help": "Proxy Host",
                            "validators": [
                                {
                                    "type": "url"
                                }
                            ]
                        },
                        {
                            "field": "port",
                            "label": "Port",
                            "type": "text",
                            "help": "Proxy Port",
                            "validators": [
                                {
                                    "type": "number",
                                    "range": [
                                        1,
                                        65535
                                    ]
                                }
                            ]
                        },
                        {
                            "field": "username",
                            "label": "Username",
                            "type": "text",
                            "defaultValue": "",
                            "help": "Username"
                        },
                        {
                            "field": "password",
                            "label": "Password",
                            "type": "text",
                            "defaultValue": "",
                            "encrypted": true,
                            "help": "Password"
                        }
                    ],
                    "options": {
                        "saveValidator": "function(formData) { if(!formData.proxy_enabled || formData.proxy_enabled === '0') {return true; } if (!formData.proxy_type) {return 'Input of \"Proxy Type\" can not be empty';} if(!formData.host) { return 'Input of \"Host\" can not be empty'; } if(!formData.port) { return 'Input of \"Port\" can not be empty'; } return true; }"
                    }
                }
            ],
            "title": "Configuration",
            "description": "Set up your add-on and configure the connection to Splunk Attack Analyzer.",
            "subDescription": {
                "text": "Don't have an API key to your Splunk Attack Analyzer tenant yet? Review the [[apiKeyDocs]] to learn how to create the API key required to set up the integration. Don't have access to Attack Analyzer yet? [[learn]].",
                "links": [
                    {
                        "slug": "apiKeyDocs",
                        "link": "https://docs.splunk.com/Documentation/AttackAnalyzer/Current/Detect/APIKey",
                        "linkText": "documentation"
                    },
                    {
                        "slug": "learn",
                        "link": "https://www.splunk.com/en_us/products/attack-analyzer.html",
                        "linkText": "Learn more"
                    }
                ]
            }
        },
        "inputs": {
            "services": [
                {
                    "name": "jobs_input",
                    "restHandlerModule": "Splunk_TA_SAA_rh_delete_checkpoint",
                    "restHandlerClass": "JobsInputDeleteCheckpoint",
                    "groups": [
                        {
                            "label": "Base Configuration",
                            "options": {
                                "isExpandable": false
                            },
                            "fields": [
                                "name",
                                "interval",
                                "index",
                                "account"
                            ]
                        },
                        {
                            "label": "Query Configuration",
                            "options": {
                                "isExpandable": true,
                                "expand": true
                            },
                            "fields": [
                                "source",
                                "username",
                                "api_key_id",
                                "since_options"
                            ]
                        },
                        {
                            "label": "Forensics Configuration",
                            "options": {
                                "isExpandable": true,
                                "expand": true
                            },
                            "fields": [
                                "ingest_forensics",
                                "forensic_components"
                            ]
                        },
                        {
                            "label": "Advanced Configuration",
                            "options": {
                                "isExpandable": true,
                                "expand": false
                            },
                            "fields": [
                                "since"
                            ]
                        }
                    ],
                    "entity": [
                        {
                            "type": "text",
                            "label": "Name",
                            "validators": [
                                {
                                    "type": "regex",
                                    "errorMsg": "Input Name must begin with a letter and consist exclusively of alphanumeric characters and underscores.",
                                    "pattern": "^[a-zA-Z]\\w*$"
                                },
                                {
                                    "type": "string",
                                    "errorMsg": "Length of input name should be between 1 and 100",
                                    "minLength": 1,
                                    "maxLength": 100
                                }
                            ],
                            "field": "name",
                            "help": "A unique name for the data input.",
                            "required": true
                        },
                        {
                            "type": "text",
                            "field": "interval",
                            "label": "Interval",
                            "validators": [
                                {
                                    "type": "regex",
                                    "errorMsg": "Interval must be either a non-negative number, CRON interval or -1.",
                                    "pattern": "^((?:-1|\\d+(?:\\.\\d+)?)|(([\\*\\d{1,2}\\,\\-\\/]+\\s){4}[\\*\\d{1,2}\\,\\-\\/]+))$"
                                },
                                {
                                    "type": "number",
                                    "range": [
                                        60,
                                        999999999999
                                    ],
                                    "errorMsg": "Interval must be between 60 and 999999999999"
                                }
                            ],
                            "help": "Time interval of the data input, in seconds.",
                            "required": true,
                            "defaultValue": "300"
                        },
                        {
                            "type": "singleSelect",
                            "label": "Connection",
                            "options": {
                                "referenceName": "account",
                                "disableonEdit": true
                            },
                            "help": "Connection to use for this input.",
                            "field": "account",
                            "required": true
                        },
                        {
                            "type": "singleSelect",
                            "field": "index",
                            "label": "Index",
                            "defaultValue": "main",
                            "options": {
                                "endpointUrl": "data/indexes?search=isInternal=0+disabled=0",
                                "denyList": "^_.*$",
                                "createSearchChoice": true
                            },
                            "validators": [
                                {
                                    "type": "regex",
                                    "errorMsg": "Index names must begin with a letter or a number and must contain only letters, numbers, underscores or hyphens.",
                                    "pattern": "^[a-zA-Z0-9][a-zA-Z0-9\\\\_\\\\-]*$"
                                },
                                {
                                    "type": "string",
                                    "errorMsg": "Length of index name should be between 1 and 80.",
                                    "minLength": 1,
                                    "maxLength": 80
                                }
                            ],
                            "help": "An index is a type of data repository. Select the index in which you want to collect the events.",
                            "required": true
                        },
                        {
                            "validators": [
                                {
                                    "type": "regex",
                                    "errorMsg": "Start Time must be the number of seconds since Unix epox.",
                                    "pattern": "^\\-[1-9]\\d*$|^\\d*$"
                                }
                            ],
                            "options": {
                                "disableonEdit": true
                            },
                            "field": "since",
                            "type": "text",
                            "required": false,
                            "label": "Backfill from Epoch",
                            "help": "Overrides Backfill value to backfill from a particular point in time. If included, it should be the number of seconds since the Unix epoch and MUST be in the past."
                        },
                        {
                            "options": {
                                "disableonEdit": true,
                                "disableSearch": true,
                                "autoCompleteFields": [
                                    {
                                        "value": "1",
                                        "label": "1 day"
                                    },
                                    {
                                        "value": "7",
                                        "label": "7 days"
                                    },
                                    {
                                        "value": "14",
                                        "label": "14 days"
                                    }
                                ]
                            },
                            "field": "since_options",
                            "type": "singleSelect",
                            "help": "Time from which to backfill jobs during initial run. By default, no backfill is performed.",
                            "label": "Backfill"
                        },
                        {
                            "field": "source",
                            "type": "singleSelect",
                            "help": "Filter jobs by submission source. Omit to include all",
                            "label": "Source",
                            "required": false,
                            "options": {
                                "disableonEdit": true,
                                "autoCompleteFields": [
                                    {
                                        "value": "ui",
                                        "label": "UI"
                                    },
                                    {
                                        "value": "api",
                                        "label": "API"
                                    }
                                ]
                            }
                        },
                        {
                            "field": "api_key_id",
                            "type": "text",
                            "options": {
                                "disableonEdit": true
                            },
                            "required": false,
                            "label": "API Key ID",
                            "help": "Filter jobs by API Key ID. Exact match only."
                        },
                        {
                            "field": "username",
                            "type": "text",
                            "options": {
                                "disableonEdit": true
                            },
                            "required": false,
                            "label": "Username",
                            "help": "Filter jobs by username. Exact match only."
                        },
                        {
                            "label": "Ingest Forensics",
                            "field": "ingest_forensics",
                            "type": "checkbox",
                            "defaultValue": 0,
                            "help": "Ingest normalized forensics corresponding to the ingested jobs"
                        },
                        {
                            "type": "multipleSelect",
                            "field": "forensic_components",
                            "label": "Forensic Components",
                            "help": "Select the forensics that should be ingested into Splunk. If this input is empty, no forensics are ingested.",
                            "defaultValue": "",
                            "options": {
                                "delimiter": "|",
                                "items": [
                                    {
                                        "value": "TLS",
                                        "label": "TLS"
                                    },
                                    {
                                        "value": "Files",
                                        "label": "Files"
                                    },
                                    {
                                        "value": "Detections",
                                        "label": "Detections"
                                    },
                                    {
                                        "value": "URLs",
                                        "label": "URLs"
                                    },
                                    {
                                        "value": "HTTP",
                                        "label": "HTTP"
                                    },
                                    {
                                        "value": "Network",
                                        "label": "Network"
                                    },
                                    {
                                        "value": "DNSRequests",
                                        "label": "DNS Requests"
                                    },
                                    {
                                        "value": "Hosts",
                                        "label": "Hosts"
                                    },
                                    {
                                        "value": "Processes",
                                        "label": "Processes"
                                    },
                                    {
                                        "value": "Strings",
                                        "label": "Strings"
                                    },
                                    {
                                        "value": "Images",
                                        "label": "Images (not recommended)"
                                    },
                                    {
                                        "value": "Screenshots",
                                        "label": "Screenshots (not recommended)"
                                    },
                                    {
                                        "value": "MitreAttacks",
                                        "label": "MITRE ATT&CKs"
                                    },
                                    {
                                        "value": "RegistryKeys",
                                        "label": "Registry Keys"
                                    },
                                    {
                                        "value": "SavedArtifacts",
                                        "label": "Saved Artifacts (not recommended)"
                                    },
                                    {
                                        "value": "MalwareFamilies",
                                        "label": "Malware Families"
                                    },
                                    {
                                        "value": "PhishedBrands",
                                        "label": "Phished Brands"
                                    },
                                    {
                                        "value": "PhishKitFamilies",
                                        "label": "PhishKit Families"
                                    }
                                ]
                            }
                        }
                    ],
                    "title": "Completed Jobs Input"
                }
            ],
            "title": "Inputs",
            "description": "Manage your data inputs",
            "table": {
                "actions": [
                    "edit",
                    "delete",
                    "clone"
                ],
                "header": [
                    {
                        "label": "Name",
                        "field": "name"
                    },
                    {
                        "label": "Connection",
                        "field": "account"
                    },
                    {
                        "label": "Interval",
                        "field": "interval"
                    },
                    {
                        "label": "Index",
                        "field": "index"
                    },
                    {
                        "label": "Status",
                        "field": "disabled"
                    }
                ],
                "moreInfo": [
                    {
                        "label": "Name",
                        "field": "name"
                    },
                    {
                        "label": "Connection",
                        "field": "account"
                    },
                    {
                        "label": "Interval",
                        "field": "interval"
                    },
                    {
                        "label": "Index",
                        "field": "index"
                    },
                    {
                        "label": "Status",
                        "field": "disabled",
                        "mapping": {
                            "true": "Disabled",
                            "false": "Enabled"
                        }
                    }
                ]
            }
        },
        "dashboard": {
            "panels": [
                {
                    "name": "default"
                }
            ]
        }
    },
    "meta": {
        "name": "Splunk_TA_SAA",
        "restRoot": "Splunk_TA_SAA",
        "version": "1.2.1",
        "displayName": "Splunk Add-on for Splunk Attack Analyzer",
        "schemaVersion": "0.0.9",
        "supportedThemes": [
            "dark",
            "light"
        ],
        "_uccVersion": "5.64.0"
    }
}
