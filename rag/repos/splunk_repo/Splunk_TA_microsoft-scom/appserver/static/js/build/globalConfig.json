                {
                    "name": "server",
                    "table": {
                        "actions": [
                            "edit",
                            "delete",
                            "clone"
                        ],
                        "header": [
                            {
                                "label": "Name",
                                "field": "name"
                            },
                            {
                                "label": "Host",
                                "field": "host"
                            },
                            {
                                "label": "Username",
                                "field": "username"
                            }
                        ]
                    },
                    "entity": [
                        {
                            "type": "text",
                            "label": "Name",
                            "validators": [
                                {
                                    "type": "string",
                                    "errorMsg": "Length of server name should be between 1 and 100",
                                    "minLength": 1,
                                    "maxLength": 100
                                },
                                {
                                    "type": "regex",
                                    "errorMsg": "Name must begin with a letter and consist exclusively of alphanumeric characters and underscores.",
                                    "pattern": "^[a-zA-Z]\\w*$"
                                }
                            ],
                            "field": "name",
                            "help": "Enter a unique name for this Server.",
                            "required": true
                        },
                        {
                            "type": "text",
                            "label": "Host",
                            "validators": [
                                {
                                    "type": "string",
                                    "errorMsg": "Length of host should be between 1 and 250",
                                    "minLength": 1,
                                    "maxLength": 250
                                },
                                {
                                    "pattern": "^[a-zA-Z0-9:][a-zA-Z0-9\\.\\-:]+$",
                                    "errorMsg": "Host is invalid",
                                    "type": "regex"
                                }
                            ],
                            "field": "host",
                            "help": "Enter a host for this Server.",
                            "required": true
                        },
                        {
                            "type": "text",
                            "label": "Username",
                            "validators": [
                                {
                                    "type": "string",
                                    "errorMsg": "Length of username should be between 1 and 250",
                                    "minLength": 1,
                                    "maxLength": 250
                                }
                            ],
                            "field": "username",
                            "help": "Enter a  Username for this Server.",
                            "required": true
                        },
                        {
                            "field": "password",
                            "label": "Password",
                            "type": "text",
                            "encrypted": true,
                            "required": true,
                            "help": "Enter the password for this Server.",
                            "validators": [
                                {
                                    "type": "string",
                                    "minLength": 1,
                                    "maxLength": 8192,
                                    "errorMsg": "Length of password should be between 1 and 8192"
                                }
                            ]
                        }
                    ],
                    "title": "Microsoft SCOM Server"
                },
