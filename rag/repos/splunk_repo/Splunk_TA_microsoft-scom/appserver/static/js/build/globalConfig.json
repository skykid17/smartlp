        "inputs": {
            "services": [
                {
                    "name": "task",
                    "hook": {
                        "src": "inputs_hook.4.1.0",
                        "type": "external"
                    },
                    "entity": [
                        {
                            "type": "text",
                            "label": "Name",
                            "validators": [
                                {
                                    "type": "regex",
                                    "errorMsg": "Input Name must begin with a letter and consist exclusively of alphanumeric characters and underscores.",
                                    "pattern": "^[a-zA-Z]\\w*$"
                                },
                                {
                                    "type": "string",
                                    "errorMsg": "Length of input name should be between 1 and 100",
                                    "minLength": 1,
                                    "maxLength": 100
                                }
                            ],
                            "field": "name",
                            "help": "Enter a unique name for the input.",
                            "required": true
                        },
                        {
                            "type": "text",
                            "label": "Description",
                            "validators": [
                                {
                                    "type": "string",
                                    "errorMsg": "Length of description should be between 1 and 8192",
                                    "minLength": 1,
                                    "maxLength": 8192
                                }
                            ],
                            "field": "description",
                            "help": "Enter a description for this input.",
                            "required": false
                        },
                        {
                            "field": "templates",
                            "label": "Template",
                            "type": "multipleSelect",
                            "required": true,
                            "options": {
                                "delimiter": "|",
                                "referenceName": "template"
                            },
                            "help": "Select template(s) for input."
                        },
                        {
                            "field": "filter_parameters",
                            "label": "Filter Parameters",
                            "type": "text",
                            "defaultValue": "CounterName IS NOT NULL",
                            "required": false,
                            "options": {
                                "display": false
                            }
                        },
                        {
                            "field": "server",
                            "label": "Server",
                            "type": "multipleSelect",
                            "required": true,
                            "options": {
                                "referenceName": "server"
                            },
                            "help": "Select server for input. Additional Servers will be used as a fallback solution"
                        },
                        {
                            "field": "interval",
                            "label": "Interval",
                            "type": "text",
                            "required": true,
                            "help": "Enter the interval in Quartz Cron format (e.g., 0 0 0 ? * *)"
                        },
                        {
                            "type": "singleSelect",
                            "label": "Index",
                            "validators": [
                                {
                                    "type": "string",
                                    "errorMsg": "Length of index name should be between 1 and 80.",
                                    "minLength": 1,
                                    "maxLength": 80
                                }
                            ],
                            "defaultValue": "default",
                            "options": {
                                "endpointUrl": "data/indexes",
                                "denyList": "^_.*$",
                                "createSearchChoice": true
                            },
                            "field": "index",
                            "required": true
                        },
                        {
                            "field": "starttime",
                            "label": "Start Time",
                            "type": "text",
                            "validators": [
                                {
                                    "type": "regex",
                                    "errorMsg": "Field 'Start Time' is not in format: YYYY-MM-DDThh:mm:ssZ.",
                                    "pattern": "^(19|20)\\d\\d-(0[1-9]|1[012])-(0[1-9]|[12][0-9]|3[01])T([01]?[0-9]|2[0-3]):[0-5][0-9]:[0-5][0-9](Z|[+-][0-5][0-9]:[0-5][0-9])$"
                                }
                            ],
                            "required": false,
                            "help": "The add-on starts collecting data with later than provided UTC time. Default: one day before current UTC time. Format: YYYY-MM-DDThh:mm:ssZ"
                        }
                    ],
                    "title": "Microsoft SCOM Inputs"
                }
            ],
